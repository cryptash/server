{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "lib": ["es2020"],
    "moduleResolution": "node",
    "declaration": true,
    "rootDir": "src",
    "outDir": "dist",
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noErrorTruncation": true,
    "resolveJsonModule": true,
    "downlevelIteration": true
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "dist", "*.json", "testing","client"]
}
